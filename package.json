{
  "name": "webbase",
  "version": "2.0.0",
  "description": "webbase 2",
  "main": "src/app.js",
  "scripts": {
    "start": "node ./src/app.js",
    "cmd": "node ./src/app.js -c",
    "dbinstall": "node ./src/app.js -i",
    "dbuninstall": "node ./src/app.js -id",
    "dbupdate": "node ./src/app.js -iu",
    "lint": "eslint ./src ./test",
    "lintautofix": "eslint --fix ./src ./test",
    "dev": "npm run lint && npx webpack --progress --mode development && node ./src/app.js",
    "devnolint": "npx webpack --progress --mode development && node ./src/app.js",
    "test": "mocha ./test",
    "build": "npx webpack --mode production"
  },
  "author": "HIER",
  "license": "MIT",
  "dependencies": {
    "@babel/preset-react": "^7.14.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "connect-session-sequelize": "^7.1.1",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "express-ws": "^5.0.2",
    "helmet": "^4.6.0",
    "jssha": "^3.2.0",
    "minimist": "^1.2.5",
    "node-cron": "^3.0.0",
    "nodemailer": "^6.7.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sequelize": "^6.6.4",
    "sqlite3": "^5.0.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
