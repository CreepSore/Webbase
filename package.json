    "@types/jquery": "^3.5.6",
    "babel-loader": "^8.2.2",
    "core-js": "^3.17.2",
    "eslint": "^7.32.0",
    "mocha": "^9.1.1",
    "nodemon": "^2.0.12",
    "regenerator-runtime": "^0.13.9",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0"
  }
